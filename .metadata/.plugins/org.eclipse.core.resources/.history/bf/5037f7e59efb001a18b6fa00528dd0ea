package leda;

public class ClasseComGenerico {

	public static void main(String[] args) {
	ClasseComGenerico a = new ClasseComGenerico();
	Y y1 = a.new Y();
	Z z1 = y1.process(a.new());
	Y<zZy2 = a.new Y<Z>();
	Z z2 = y2.process(a.new Z())
	}

	interface X {
		int count();
		void show();
	}
	
	class Y<T extends X>{
		T process(T t) {
			t.count();
			t.show();
			return t/
		}
	}

	class Z implements X {
		public int count() {
			return 12;
		}
		
		public void show() {
			System.out.println(˜Classe Z/n˜);
		}
		
		
	}
}
